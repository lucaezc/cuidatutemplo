<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.holysoft.cuidatutemplo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.holysoft.cuidatutemplo.App">
        <activity
            android:name="com.holysoft.cuidatutemplo.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name="com.holysoft.cuidatutemplo.DetalleAlimentoActivity" />
        <activity android:name="com.holysoft.cuidatutemplo.ResultadoBusquedaActivity" />
        <activity android:name="com.holysoft.cuidatutemplo.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>