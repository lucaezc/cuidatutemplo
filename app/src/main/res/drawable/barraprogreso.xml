<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dp"/>
                <gradient
                    android:startColor="@color/progress_inicio"
                    android:centerColor="@color/progress_medio"
                    android:endColor="@color/progress_final"
                    android:angle="180" />
            </shape>
        </clip>
    </item>

</layer-list>
